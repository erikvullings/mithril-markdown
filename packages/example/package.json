{
  "name": "example",
  "private": true,
  "version": "0.0.1",
  "description": "Example project voor the mithril-markdown library.",
  "scripts": {
    "start": "parcel index.html --out-dir ../../docs",
    "build:domain": "rm -fr ../../docs && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/mithril-markdown && cp favicon.ico ../../docs/",
    "link": "npm link mithril-markdown",
    "build": "rm -fr dist && parcel build index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "example",
    "mithril",
    "typescript",
    "markdown",
    "editor"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "mithril": "next",
    "mithril-markdown": "^0.1.5",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril-materialized": "^0.11.8"
  },
  "devDependencies": {
    "@types/materialize-css": "^1.0.6",
    "@types/mithril": "^1.1.16",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.4.5"
  }
}
