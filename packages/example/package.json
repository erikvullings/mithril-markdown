{
  "name": "example",
  "private": true,
  "version": "0.0.1",
  "description": "Example project voor the mithril-markdown library.",
  "scripts": {
    "build:domain": "rm -fr ../../docs && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/mithril-markdown && cp favicon.ico ../../docs/",
    "start": "webpack serve --env development --open",
    "build": "webpack --env production",
    "clean": "rimraf dist .cache"
  },
  "keywords": [
    "example",
    "mithril",
    "typescript",
    "markdown",
    "editor"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "marked": "^2.1.3",
    "material-icons": "^1.1.0",
    "materialize-css": "^1.0.0",
    "mithril": "2.0.4",
    "mithril-markdown": "workspace:*",
    "mithril-materialized": "^0.19.0",
    "slimdown-js": "0.4.1"
  },
  "devDependencies": {
    "@types/marked": "^2.0.4",
    "@types/materialize-css": "^1.0.11",
    "@types/mithril": "^2.0.8",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv-webpack": "^7.0.3",
    "html-webpack-plugin": "^5.3.2",
    "html-webpack-tags-plugin": "^3.0.1",
    "mini-css-extract-plugin": "^2.1.0",
    "rimraf": "3.0.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
